    "version": "1.0.0",
    "keywords": ["laravel", "magento", "soap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kyle Martin",
            "email": "michael@tinyrocket.co"
        },
        {
            "name": "Adam Greer",
            "email": "adam.greer@netmatters.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
